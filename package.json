{
  "name": "@vaened/mui-search-engine",
  "type": "module",
  "version": "1.0.0",
  "description": "React + Material UI component library for building advanced search and filtering.",
  "keywords": [
    "react",
    "mui",
    "material-ui",
    "search",
    "search-builder",
    "filters",
    "typescript",
    "ui",
    "component-library",
    "frontend"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --dts",
    "play": "vite --config vite.config.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "@mui/material": "^7.3",
    "react": ">=19.2",
    "react-dom": ">=19.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tabler/icons-react": "^3.35.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.4",
    "happy-dom": "^20.0.10",
    "rimraf": "^5",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5",
    "typescript": "^5.9",
    "vite": "^7.1.9",
    "vitest": "^4.0.13"
  },
  "authors": [
    {
      "name": "Enea Dhack",
      "email": "enea.so@live.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaened/mui-search-engine"
  },
  "bugs": {
    "url": "https://github.com/vaened/mui-search-engine/issues"
  },
  "homepage": "https://github.com/vaened/mui-search-engine#readme",
  "packageManager": "pnpm@10.16.0"
}
